<%= form_for @schedule, :html => { :class => "form-horizontal schedule" } do |f| %>

    <% if @schedule.errors.any? %>
        <div id="error_expl" class="panel panel-danger">
          <div class="panel-heading">
            <h3 class="panel-title"><%= pluralize(@schedule.errors.count, "error") %> prohibited this course from being saved:</h3>
          </div>
          <div class="panel-body">
            <ul>
              <% @schedule.errors.full_messages.each do |msg| %>
                  <li><%= msg %></li>
              <% end %>
            </ul>
          </div>
        </div>
    <% end %>

    <div class="form-group">
      <%= f.label :users_id, :class => 'control-label col-lg-2' %>
      <div class="col-lg-10">
        <%= f.text_field :users_id, :class => 'form-control' %>
      </div>
      <%=f.error_span(:users_id) %>
    </div>
    <div class="form-group">
      <%= f.label :courses_id, :class => 'control-label col-lg-2' %>
      <div class="col-lg-10">
        <%= f.text_field :courses_id, :class => 'form-control' %>
      </div>
      <%=f.error_span(:courses_id) %>
    </div>
    <div class="form-group">
      <%= f.label :semester, :class => 'control-label col-lg-2' %>
      <div class="col-lg-10">
        <%= f.text_field :semester, :class => 'form-control' %>
      </div>
      <%=f.error_span(:semester) %>
    </div>
    <div class="form-group">
      <%= f.label :year, :class => 'control-label col-lg-2' %>
      <div class="col-lg-10">
        <%= f.text_field :year, :class => 'form-control' %>
      </div>
      <%=f.error_span(:year) %>
    </div>
    <div class="form-group">
      <%= f.label :complete, :class => 'control-label col-lg-2' %>
      <div class="col-lg-10">
        <%= f.collection_select :complete, User::COMPLETED, :to_s, :to_s, :include_blank =>false, :class => 'control-label col-lg-2' %>
      </div>
      <%=f.error_span(:complete) %>
    </div>


    <div class="form-group">
      <div class="col-lg-offset-2 col-lg-10">
        <%= f.submit nil, :class => 'btn btn-primary' %>
        <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                    users_path, :class => 'btn btn-default' %>
      </div>
    </div>

<% end %>
